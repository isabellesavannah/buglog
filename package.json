{
  "name": "buglog",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run setup:client && npm run setup:server && npm run build:client",
    "setup:client": "cd buglog.client && npm i",
    "setup:server": "cd buglog.server && npm i",
    "build:client": "npm run --prefix buglog.client build",
    "start": "node buglog.server/index.js"
  },
  "engines": {
    "node": "^14.15.4"
  }
}
